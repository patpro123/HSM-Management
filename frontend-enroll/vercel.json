{
  "buildCommand": "cd frontend-enroll && npm install && npm run build",
  "outputDirectory": "frontend-enroll/dist",
  "installCommand": "npm install --prefix frontend-enroll",
  "devCommand": "cd frontend-enroll && npm run dev",
  "rewrites": [
    { "source": "/(.*)", "destination": "/index.html" }
  ]
}
